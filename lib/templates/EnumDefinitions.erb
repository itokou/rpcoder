/* generated by rpcoder */

namespace <%= name_space %>
{
	<%- enums.each do |enum| -%>
	<%- if enum.description -%>
	/// <summary>
	/// <%= enum.description %>
	/// </summary>
	<%- end -%>
	<%- if enum.flags? -%>
	[Flags]
	<%- end -%>
	public enum <%= enum.name.to_s.camelize %>
	{
		<%- enum.constants.each do |i| -%>
		<%- if i.description -%>
		/// <summary>
		/// <%= i.description %>
		/// </summary>
		<%- end -%>
		<%= i.name.to_s.camelize %> = <%= i.num %>,
		<%- end -%>
	}

	<%- end -%>
}
